SELECT
    e.id,
    e.first_name,
    e.last_name
FROM
    Employee e
WHERE
    e.team_id IS NULL; -- Si l'attribut de jointure est dans Employee (le plus probable)
-- OU, si la mod√©lisation implique un champ NULL dans Team (moins probable avec le MCD)
